{
  "name": "slider",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "better-npm-run dev",
    "demo": "better-npm-run dev:demo",
    "prod": "better-npm-run prod",
    "test": "better-npm-run test",
    "singleTest": "better-npm-run test:single"
  },
  "betterScripts": {
    "test": {
      "command": "karma start karma.confing.js",
      "env": {
        "NODE_ENV": "testing"
      }
    },
    "test:single": {
      "command": "karma start karma.confing.js --single-run --browsers PhantomJS",
      "env": {
        "NODE_ENV": "testing"
      }
    },
    "dev": {
      "command": "webpack-dev-server --watch --progress --colors --config webpack/dev.config.js",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "dev:demo": {
      "command": "webpack --progress --colors --config webpack/dev.config.js",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "prod": {
      "command": "webpack --progress --display-error-details --colors --config webpack/prod.config.js",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/prodderman/Slider.git"
  },
